{
	"$schema": "./node_modules/@angular/cli/lib/config/schema.json",
	"version": 1,
	"newProjectRoot": "projects",
	"projects": {
		"never-land": {
			"projectType": "library",
			"root": "projects/never-land",
			"sourceRoot": "projects/never-land/src",
			"prefix": "lib",
			"architect": {
				"build": {
					"builder": "@angular-devkit/build-ng-packagr:build",
					"options": {
						"tsConfig": "projects/never-land/tsconfig.lib.json",
						"project": "projects/never-land/ng-package.json"
					},
					"configurations": {
						"production": {
							"tsConfig": "projects/never-land/tsconfig.lib.prod.json"
						}
					}
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"main": "projects/never-land/src/test.ts",
						"tsConfig": "projects/never-land/tsconfig.spec.json",
						"karmaConfig": "projects/never-land/karma.conf.js"
					}
				},
				"lint": {
					"builder": "@angular-devkit/build-angular:tslint",
					"options": {
						"tsConfig": [
							"projects/never-land/tsconfig.lib.json",
							"projects/never-land/tsconfig.spec.json"
						],
						"exclude": [
							"**/node_modules/**"
						]
					}
				}
			}
		}
	},
	"cli": {
		"analytics": false
	},
	"defaultProject": "never-land"
}